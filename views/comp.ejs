    </div>

    <!-- itemselected -->

    <div class="dishinfo">

        <div>
            <img class="dishimg" src=<%= list[0].picture %> alt="">
        </div>

            <p class="dishname" style="font-size: 2.5em; font-family:'Times New Roman', Times, serif;margin-top: 25px;margin-bottom: 20px;">
                <strong>
                    <%=list[0].name%>
                </strong>
            </p>
        <!-- <p style="margin-bottom:10px;">Categorize By</p> -->
    </div>
    <div class="category">
        <form method="post" action="/rating"><button type="submit" name="category" value="rating" class="button">Rating</button></form>
        <form method="post" action="/finalselecteditem"><button type="submit" name="category" value="rating" class="button">Price</button></form>
        <form method="post" action="/distance"><button type="submit" name="category" value="rating" class="button">Distance</button></form>
    </div>

    <!-- comparison -->

    <div class="comparison">
        <% for(i=0;i<list.length;i++){%>
            <div style="display: flex; justify-content: center;">
            <div class="fullitem">

                <div class="item-image"><img class="item-img-style" 
                        src= <%=list[i].picture%> alt="item image"></div>
                <% if(list[i].rating_max_id==0){ %>
                <div class="item-service-logo"><img style=" height: 65px;width: 40px;" src="css/Swiggy_logo.svg.png"
                        alt="item service"></div>
                <% }else{ %>
                <div class="item-service-logo"><img style=" height: 40px;width: 100px;" src="css/Zomato_logo1.png"
                        alt="item service"></div>
                <% }%>
                <div class="item-price">
                    <p style="font-size: 2.5rem; margin: 0px;">
                        <%=list[i].price%>
                    </p>
                </div>
                <div class="rest-name">
                    <p style="font-size: 1.5em;">
                        <%=list[i].restraunt%>
                    </p>
                </div>
                <div class="extra-item-info">
                    <p style="font: 1em;">
                        <%=list[i].time%> <span> min</span>
                    </p>
                    <p>
                        <%=list[i].rating%>
                        <img style="height: 10px;" src="css/starlogo.png">
                    </p>
                </div>
                <div class="rest-item-name">
                    <p style="padding-left: 41px;font-size: 1.2em;">J<%=list[i].name%>
                    </p>
                </div>
                <div class="item-description">
                    <p style="padding-left: 41px;font-size: 1em;">
                        <%=list[i].description%>
                    </p>
                </div>
            </div>
            </div>
            <%}%>

